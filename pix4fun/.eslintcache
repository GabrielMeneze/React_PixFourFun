p>Você ainda pode escolher {limitador} imagens</p>}\r\n                        <div className=\"escolherdnv\">\r\n                            <button\r\n                                className=\"Btn\"\r\n                                onClick={refbtnEscolher}\r\n                            >ESCOLHER OUTRA IMAGEM</button>\r\n                            <button\r\n                                className=\"Btn\"\r\n                                onClick={uparImg}\r\n                            >SALVAR E ENVIAR</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <input\r\n                    ref={inputCortar}\r\n                    style={{ display: 'none' }}\r\n                />\r\n                <Button\r\n                    onClick={refCortar}>cortar</Button>\r\n            </div>\r\n            </>\r\n                    ) : null}\r\n\r\n            <div id=\"contact\" />\r\n            <div id=\"doubt\" />\r\n            <Footer />\r\n        </div >\r\n    )\r\n}\r\nexport default UploadImagem;\r\n\r\n\r\n",{"ruleId":"24","severity":1,"message":"25","line":10,"column":27,"nodeType":"26","messageId":"27","endLine":10,"endColumn":33},{"ruleId":"24","severity":1,"message":"28","line":10,"column":35,"nodeType":"26","messageId":"27","endLine":10,"endColumn":39},{"ruleId":"24","severity":1,"message":"29","line":12,"column":10,"nodeType":"26","messageId":"27","endLine":12,"endColumn":15},{"ruleId":"24","severity":1,"message":"30","line":12,"column":17,"nodeType":"26","messageId":"27","endLine":12,"endColumn":20},{"ruleId":"24","severity":1,"message":"31","line":17,"column":17,"nodeType":"26","messageId":"27","endLine":17,"endColumn":23},{"ruleId":"24","severity":1,"message":"32","line":23,"column":21,"nodeType":"26","messageId":"27","endLine":23,"endColumn":33},{"ruleId":"24","severity":1,"message":"33","line":29,"column":12,"nodeType":"26","messageId":"27","endLine":29,"endColumn":17},{"ruleI